# 环境配置文件
# 复制此文件为 .env 并填入你的实际配置值

# Google AI API配置
GOOGLE_AI_API_KEY=your_google_ai_api_key_here

# 服务器配置
PORT=3000
NODE_ENV=development

# 数据库配置
DB_PATH=./data/diary.db

# 日志配置
LOG_LEVEL=info

# 提醒服务配置
REMINDER_CHECK_INTERVAL=*/5 * * * *  # 每5分钟检查一次提醒

# 应用配置
APP_NAME=Dear Diary
APP_VERSION=1.0.0